package com.Daniel.response;

import com.Daniel.entity.Departamento;

public class DepartamentoResponse {
	
	private String nit;
	private String nombre;
	
	public DepartamentoResponse(Departamento compania) {
		this.nit = compania.getNit();
		this.nombre = compania.getNombre();
	}

	public String getNit() {
		return nit;
	}

	public void setNit(String nit) {
		this.nit = nit;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	
}
