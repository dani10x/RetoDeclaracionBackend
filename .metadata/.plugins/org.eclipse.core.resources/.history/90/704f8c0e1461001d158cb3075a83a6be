package com.Daniel.response;

import com.Daniel.entity.Traccion;

public class TraccionResponse {
	
	private String id_traccion;
	private String nombre;
	
	public TraccionResponse(Traccion traccion) {
		this.id_traccion = traccion.getId_traccion();
		this.nombre = traccion.getNombre();
	}

	public String getId_traccion() {
		return id_traccion;
	}

	public void setId_traccion(String id_traccion) {
		this.id_traccion = id_traccion;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
}
