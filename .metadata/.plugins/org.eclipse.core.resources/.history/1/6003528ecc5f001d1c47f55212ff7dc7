package com.Daniel.response;

import com.Daniel.entity.Caja;

public class CarroceriaResponse {
	
	private String id_caja;
	private String nombre;
	
	public CarroceriaResponse(Caja caja) {
		this.id_caja = caja.getId_caja();
		this.nombre = caja.getNombre();
	}
	
	public String getId_caja() {
		return id_caja;
	}
	public void setId_caja(String id_caja) {
		this.id_caja = id_caja;
	}
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	
	
}
